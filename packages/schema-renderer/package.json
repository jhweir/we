    "./solid": {
      "types": "./dist/solid/index.d.ts",
      "import": "./dist/solid/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "solid-js": "^1.9.5"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.15",
    "@we/cli": "workspace:*",
    "esbuild-plugin-solid": "^0.6.0",
    "jsdom": "^27.2.0",
    "solid-js": "^1.9.5",
    "tsup": "^8.5.1",
    "vitest": "^4.0.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}

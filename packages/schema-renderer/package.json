{
  "name": "@we/schema-renderer",
  "version": "0.1.0",
  "description": "UI renderer for WE apps",
  "author": "James Weir",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:steps": "tsup",
    "build": "we-build",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "types": "dist/index.d.ts",
  "exports": {
    "./shared": {
      "types": "./dist/shared/index.d.ts",

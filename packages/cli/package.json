{
  "name": "@we/cli",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "bin": {
    "we-build": "./dist/we-build.js",
    "we-banner": "./dist/we-banner.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "prepare": "pnpm run build:steps",
    "build:steps": "tsup --config tsup.config.ts",
    "build": "[ -f ./dist/we-build.js ] && node ./dist/we-build.js || pnpm run build:steps"
  }
}

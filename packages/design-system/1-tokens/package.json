{
  "name": "@we/tokens",
  "version": "0.1.0",
  "description": "Design tokens for WE",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./css": "./dist/css/index.css",
    "./css/*": "./dist/css/*.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "design-tokens",
    "css",
    "design-system",
    "tokens"
  ],
  "author": "James Weir",
  "license": "MIT",
  "scripts": {
    "build:steps": "tsup --config tsup.config.ts ",
    "build": "we-build"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@we/cli": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
